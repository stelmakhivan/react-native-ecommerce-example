{
  "name": "ecommerce",
  "version": "0.0.1",
  "private": true,
  "author": "Ivan Stelmakh <stelmakh.developer@gmail.com>",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "deps:install": "npm run packages:install && npm run pods:install",
    "deps:uninstall": "npm run packages:uninstall && npm run pods:uninstall",
    "deps:reinstall": "npm run deps:uninstall && npm run deps:install",
    "packages:install": "npm install",
    "packages:uninstall": "rm -rf node_modules/",
    "packages:reinstall": "npm run packages:uninstall && npm run packages:install",
    "pods:install": "bundle exec pod install --project-directory=ios",
    "pods:uninstall": "rm -rf ios/Pods/",
    "pods:reinstall": "npm run pods:uninstall && npm run pods:install",
    "gems:install": "bundle install --path vendor/bundle",
    "gems:uninstall": "rm -rf vendor/ .bundle/",
    "gems:reinstall": "npm run gems:uninstall && npm run gems:install",
    "prepare": "husky install",
    "generate:component": "ts-node ./generate_component && git add ."
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/material-bottom-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "inquirer": "^8.1.0",
    "lodash": "^4.17.21",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-paper": "^4.12.2",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^10.1.1",
    "@tsconfig/react-native": "^2.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.18.21",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "branch-name-lint": "^2.0.0",
    "commitizen": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "engineStrict": true
}
